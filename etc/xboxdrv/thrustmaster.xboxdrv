[xboxdrv]
evdev=/dev/input/by-id/usb-Thrustmaster_Thrustmaster_dual_analog_3.2-event-joystick
silent=true
deadzone=4000
deadzone-trigger=15
detach-kernel-driver=true
force-feedback=1
mimic-xpad=true

[axismap]
-Y1=Y1
-Y2=Y2

[evdev-absmap]
ABS_X=x1
ABS_Y=y1
ABS_RZ=x2
ABS_THROTTLE=y2
ABS_HAT0X=dpad_x
ABS_HAT0Y=dpad_y

[evdev-keymap]
BTN_SOUTH=a
BTN_EAST=x
BTN_C=b
BTN_NORTH=y
BTN_WEST=lb
BTN_Z=lt
BTN_TL=rb
BTN_TR=rt
BTN_TL2=back
BTN_TR2=start
BTN_SELECT=tl
BTN_START=tr
